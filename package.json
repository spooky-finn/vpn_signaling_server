  "main": "dist/src/main.js",
  "scripts": {
    "start": "npm run build && node ./dist/src/main.js",
    "dev": "tsc -p tsconfig.json --watch & node --watch ./dist/src/main.js",
    "build": "tsc -p tsconfig.json",
    "test": "npm run build && node --test ./dist/**/*.test.js",
