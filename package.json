  "description": "",
  "dependencies": {
    "commander": "^14.0.0",
    "dotenv": "^17.2.0",
    "kysely": "^0.28.3",
    "kysely-node-sqlite": "^1.1.0",
    "node-telegram-bot-api": "^0.66.0",
    "pino": "^9.7.0",
    "pino-pretty": "^13.0.0",
